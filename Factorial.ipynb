      "source": [
        "n = int(input(\"Enter a no.:\"))\n",
        "def factorial(n):\n",
        "  assert n >= 0 and int(n) ==n, \"The no. must be a +ve integer only\"\n",
        "  if n in [0,1]:\n",
        "    return 1\n",
        "  else:  \n",
        "    return n * factorial(n-1)\n",
        "print(factorial(n))"
